-- CREATE MASTER
Select a.*, 
b.name as pok_name, b.abilities, b.against_bug, b.against_dark, b.against_dragon, b.against_electric, b.against_fairy, b.against_fight, 
b.against_fire, b.against_flying, b.against_ghost, b.against_grass, b.against_ground, b.against_ice, b.against_normal, 
b.against_poison, b.against_psychic, b.against_rock, b.against_steel, b.against_water, b.base_egg_steps, 
b.base_happiness, b.base_total, b.capture_rate, b.classfication, b.percentage_male, 
b.experience_growth, b.height_m, b.weight_kg, b.pokedex_number
INTO Pokemon_Master
from pokemon a Left Outer Join pokemon_more_characteristric b On lower(a.name) = Lower(b.name) 


UPDATE pokMaster SET
	pokMaster.pok_name = pokMore.name, pokMaster.abilities = pokMore.abilities, pokMaster.against_bug = pokMore.against_bug,
	pokMaster.against_dark = pokMore.against_dark, pokMaster.against_dragon = pokMore.against_dragon, pokMaster.against_electric = pokMore.against_electric,
	pokMaster.against_fairy = pokMore.against_fairy, pokMaster.against_fight = pokMore.against_fight, pokMaster.against_fire = pokMore.against_fire,
	pokMaster.against_flying = pokMore.against_flying, pokMaster.against_ghost = pokMore.against_ghost, pokMaster.against_grass = pokMore.against_grass,
	pokMaster.against_ground = pokMore.against_ground, pokMaster.against_ice = pokMore.against_ice, pokMaster.against_normal = pokMore.against_normal, 
	pokMaster.against_poison = pokMore.against_poison, pokMaster.against_psychic = pokMore.against_psychic, pokMaster.against_rock = pokMore.against_rock,
	pokMaster.against_steel = pokMore.against_steel, pokMaster.against_water = pokMore.against_water, pokMaster.base_egg_steps = pokMore.base_egg_steps,
	pokMaster.base_happiness = pokMore.base_happiness, pokMaster.base_total = pokMore.base_total, pokMaster.capture_rate = pokMore.capture_rate,
	pokMaster.classfication = pokMore.classfication, pokMaster.percentage_male = pokMore.percentage_male, pokMaster.experience_growth = pokMore.experience_growth,
	pokMaster.height_m = pokMore.height_m, pokMaster.weight_kg = pokMore.weight_kg, pokMaster.pokedex_number = pokMore.pokedex_number
FROM Pokemon_Master pokMaster, pokemon_more_characteristric pokMore
WHERE pokMaster.seq=230 And pokMore.pokedex_number=212

